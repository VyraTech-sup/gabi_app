{
  "expo": {
    "name": "AlmaSense",
    "slug": "all-mind",
    "version": "1.0.0",
    "orientation": "portrait",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3A5A6C"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vyratech.almasense",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSUserTrackingUsageDescription": "Este app n√£o rastreia seus dados."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3A5A6C"
      },
      "permissions": [
        "WAKE_LOCK"
      ],
      "package": "com.vyratech.almasense",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#3A5A6C"
        }
      ],
      "@react-native-community/datetimepicker",
      "react-native-iap"
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#3A5A6C"
    },
    "extra": {
      "eas": {
        "projectId": "db76ce10-369f-4780-95fc-113782270fe4"
      }
    },
    "owner": "vyratech"
  }
}
