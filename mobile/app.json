{
  "expo": {
    "name": "All Mind",
    "slug": "all-mind",
    "version": "1.0.0",
    "orientation": "portrait",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3A5A6C"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vyratech.allmind",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3A5A6C"
      },
      "permissions": [
        "WAKE_LOCK"
      ],
      "package": "com.vyratech.allmind",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#3A5A6C"
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#3A5A6C"
    },
    "extra": {
      "eas": {
        "projectId": "7f055645-77e6-4435-88dd-cc7e4f084b45"
      },
      "privacyPolicyUrl": "https://allmindpp-dwyydjyk.manus.space"
    },
    "owner": "gabiartz",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/7f055645-77e6-4435-88dd-cc7e4f084b45"
    }
  }
}
